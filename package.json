{
    "name": "vue-custom-form",
    "version": "0.1.2",
    "description": "custom form",
    "author": "zhangshuhao",
    "private": false,
    "license": "ISC",
    "keywords": [
        "component",
        "vue",
        "form",
        "element-ui"
    ],
    "homepage": "https://github.com/gudongz/vue-custom-form",
    "main": "dist/DesignForm.common.js",
    "bugs": {
        "url" : "https://github.com/gudongz/vue-custom-form/issues",
        "email": "1579542557@qq.com"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "lint": "vue-cli-service lint",
        "dev": "vue-cli-service serve --modern",
        "build": "vue-cli-service build",
        "build-bundle": "vue-cli-service build --target lib --name DesignForm ./src/index.js"
    },
    "dependencies": {
        "axios": "^0.17.0",
        "element-ui": "^2.11.0",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "md5": "^2.2.1",
        "vue": "^2.6.6",
        "vue-router": "^3.0.1",
        "vuedraggable": "^2.23.0",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.4.0",
        "@vue/cli-plugin-eslint": "^3.4.0",
        "@vue/cli-service": "^3.4.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "vue-cli-plugin-element": "^1.0.0",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "indent": [
                "error",
                4
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
